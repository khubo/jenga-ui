{
        "name": "@jenga-ui/hooks",
        "version": "0.1.1",
        "license": "MIT",
        "description": "Responsive and accessible React UI components built with TastyCSS",
        "keywords": [
                "react",
                "jenga-ui",
                "ui",
                "design-system",
                "react-components",
                "uikit",
                "accessible",
                "components",
                "tastycss",
                "library",
                "design-system"
        ],
        "main": "./dist/index.js",
        "module": "./dist/index.mjs",
        "types": "./dist/index.d.ts",
        "files": [
                "dist/**"
        ],
        "sideEffects": false,
        "scripts": {
                "build": "tsup src/index.ts --format esm,cjs --dts --external react",
                "dev": "tsup src/index.ts --format esm,cjs --watch --dts --external react",
                "lint": "TIMING=1 eslint src/**/*.ts* --fix",
                "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
        },
        "dependencies": {
                "@jenga-ui/utils": "*"
        },
        "devDependencies": {
                "@jenga-ui/tsconfig": "*",
                "@types/react": "^17.0.38",
                "@types/react-dom": "^17.0.11",
                "node-abort-controller": "^3.0.1",
                "tsup": "^6.2.3",
                "typescript": "^4.5.4"
        },
        "publishConfig": {
                "access": "public"
        },
        "peerDependencies": {
                "react": "^17.0.2",
                "react-dom": "^17.0.2"
        },
        "homepage": "https://github.com/OutpostHQ/jenga-ui",
        "repository": {
                "type": "git",
                "url": "git+https://github.com/OutpostHQ/jenga-ui.git"
        },
        "bugs": {
                "url": "https://github.com/OutpostHQ/jenga-ui/issues"
        }
}
