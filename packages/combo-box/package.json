{
  "name": "@jenga-ui/combo-box",
  "version": "0.1.1",
  "license": "MIT",
  "description": "Jenga-UI React combo-box component",
  "keywords": [
    "combo-box",
    "react",
    "jenga-ui",
    "ui"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --external react",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts --external react",
    "lint": "TIMING=1 eslint src/**/*.ts* --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@jenga-ui/tsconfig": "*",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "tsup": "^6.2.3",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@jenga-ui/form": "*",
    "@jenga-ui/layout": "*",
    "@jenga-ui/overlays": "*",
    "@jenga-ui/providers": "*",
    "@jenga-ui/select": "*",
    "@jenga-ui/text-input": "*",
    "@jenga-ui/utils": "*",
    "@react-aria/button": "^3.6.1",
    "@react-aria/combobox": "^3.4.1",
    "@react-aria/i18n": "^3.6.0",
    "@react-aria/interactions": "^3.11.0",
    "@react-aria/overlays": "^3.10.1",
    "@react-aria/utils": "^3.13.3",
    "@react-stately/collections": "^3.4.3",
    "@react-stately/combobox": "^3.2.1",
    "@react-types/combobox": "^3.5.3",
    "@react-types/shared": "^3.14.1",
    "styled-components": "^5.3.0",
    "tastycss": "^0.8.6"
  },
  "homepage": "https://github.com/OutpostHQ/jenga-ui",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OutpostHQ/jenga-ui.git"
  },
  "bugs": {
    "url": "https://github.com/OutpostHQ/jenga-ui/issues"
  }
}
