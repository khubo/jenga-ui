{
  "name": "@jenga-ui/form",
  "version": "0.1.1",
  "description": "Jenga-UI React form component",
  "keywords": [
    "form",
    "react",
    "jenga-ui",
    "ui",
    "clickable",
    "tabbable"
  ],
  "files": [
    "dist",
    "types"
  ],
  "repository": "OutpostHQ/jenga-ui",
  "license": "MIT",
  "bugs": "https://github.com/OutpostHQ/jenga-ui/issues",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "exports": {
    ".": {
      "require": "./index.ts",
      "default": "./index.ts"
    }
  },
  "types": "types/index.d.ts",
  "typings": "types/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "prebuild": "rimraf dist && rimraf types",
    "postbuild": "yarn build:types",
    "build:types": "npx tsc -b --verbose"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@vitejs/plugin-react": "^2.0.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.4",
    "vite": "^3.0.4"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@react-spectrum/utils": "^3.7.2",
    "@react-stately/collections": "^3.4.2",
    "email-validator": "^2.0.4",
    "tastycss": "^0.7.9",
    "valid-url": "^1.0.9"
  }
}
